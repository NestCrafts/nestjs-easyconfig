{
  "name": "nestjs-easyconfig",
  "version": "2.4.0",
  "description": "Add configs to nestjs with auto type parsing",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "scripts": {
    "commit": "npx cz",
    "build": "rm -rf ./dist && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint-fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test:watch": "jest --watch",
    "test": "jest --config ./test/jest.json",
    "coverage": "jest --coverage",
    "coveralls": "yarn run coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "nestjs",
    "config",
    "easyconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rubiin/nestjs-easyconfig"
  },
  "bugs": "https://github.com/rubiin/nestjs-easyconfig/issues",
  "dependencies": {
    "dotenv": "^10.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/testing": "8.0.4",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.3",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "husky": "^7.0.1",
    "jest": "27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "tsc-watch": "4.4.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "main": "dist/index.js"
}
